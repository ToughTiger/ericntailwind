{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "arbermustafa/filament-google-charts-widgets": "^2.0",
        "aws/aws-sdk-php": "^3.323",
        "bezhansalleh/filament-shield": "^3.3",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "brian2694/laravel-toastr": "^5.59",
        "filament/filament": "3.2",
        "filament/tables": "*",
        "guzzlehttp/guzzle": "^7.9",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.18",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "3.0",
        "openai-php/laravel": "^0.11.0",
        "santosh/filament-ai-tools": "dev-master",
        "spatie/laravel-analytics": "^5.5",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-markdown": "^2.5",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-sitemap": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
           
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {  
        "laravel": {
            "providers": [
                "Santosh\\FilamentAiTools\\FilamentAiToolsServiceProvider"
                         ],
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
    {
        "type": "path",
        "url": "./packages/santosh/filament-ai-tools"
    }
],
    "minimum-stability": "stable",
    "prefer-stable": true
}
